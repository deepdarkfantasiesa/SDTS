{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MySQL": "server=mysql-test;port=3306;user=root;password=123456;database=userdb;charset=utf8mb4;ConnectionReset=false;",
  "SQLServer": "Server=192.168.18.107; Database=db.user; Trusted_Connection=False; uid=sa; pwd=Zaq12wsX!@#; MultipleActiveResultSets=true;TrustServerCertificate=true",
  "RabbitMQ": {
    //"HostName": "host.docker.internal",
    "HostName": "rabbitmq_test",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "user_queue"
  },
  "kafka": "192.168.18.107:9092",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5000"
      },
      "Https": {
        "Url": "https://+:5001",
        "Certificate": {
          "Path": "./cert.pfx",
          "Password": "MyPassword",
          "AllowInvalid": true
        }
      },
      "Http2": {
        "Url": "http://+:5002",
        //设置为Http2的话consul无法健康检查但gRPC可通过http5002端口调用；设置为Http1AndHttp2的话gRPC通过http5002调用，但consul可健康检查
        //默认情况下还是http1.1；需要在ConfigureKestrel配置监听5002使用https才能让grpc以https5002调用
        "Protocols": "Http1AndHttp2"
      }
    }
    //,"EndpointDefaults": {"Protocols": "Http1AndHttp2AndHttp3"}
  },
  "ConsulRegisterOptions": {
    //"Address": "http://host.docker.internal:8500",
    "Address": "http://badger:8500",
    "HealthCheck": "/healthcheck",
    "Name": "User",
    "Ip": "host.docker.internal",
    "Port": "5002"
  },
  "Redis": {
    "Configuration": "host.docker.internal:16379",
    "InstanceName": "user_"
  },
  "Redis_Multiplexer": "some-redis:6379"
}
